{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["jest"],
  "extends": ["standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "space-before-function-paren": "off",
    "prefer-const": "off",
    "no-unused-vars": "warn",
    "no-trailing-spaces": "off",
    "no-empty": "off",
    "indent": "off",
    "quotes": ["error", "single"],
    "camelcase": "off"
  }
}
